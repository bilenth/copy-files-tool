    <UseWPF>true</UseWPF>
    <ApplicationIcon>CopyFIles.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Content Include="CopyFIles.ico" />
  </ItemGroup>

  <ItemGroup>
    <None Update="CopyFIles.ico">
